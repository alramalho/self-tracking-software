{
  "name": "@tsw/prisma",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": {
      "browser": "./client.ts",
      "default": "./index.ts"
    },
    "./client": "./client.ts",
    "./server": "./server.ts",
    "./types": "./types/index.ts",
    "./generated/prisma/sql/*": "./generated/prisma/sql/*.js"
  },
  "scripts": {
    "build": "prisma generate",
    "db:generate": "prisma generate",
    "db:generate:sql": "prisma generate --sql",
    "db:reset": "tsx reset-db.ts",
    "reset-db": "pnpm db:reset",
    "db:push": "prisma db push",
    "db:seed": "tsx seed.ts",
    "seed": "pnpm db:seed",
    "migrate-prod-to-dev": "tsx migrate_prod_to_dev.ts",
    "clean": "rm -rf node_modules",
    "db:migrate": "dotenv -e .env -- prisma migrate dev --skip-generate",
    "db:deploy": "prisma migrate deploy",
    "db:status": "prisma migrate status",
    "prod:db:status": "dotenv -e .env.prod -- prisma migrate status"
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "dotenv": "^16.3.1",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "prisma": "^6.13.0",
    "tsx": "^3.14.0"
  }
}
