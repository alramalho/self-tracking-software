{
  "name": "backend-node",
  "version": "1.0.0",
  "description": "Node.js backend for tracking.so using Express and Prisma",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "clean": "rm -rf dist && rm -rf node_modules",
    "dev": "dotenv -e .env -- tsx watch src/index.ts",
    "start": "tsx src/index.ts",
    "db:seed": "dotenv -e .env -- tsx scripts/seed.ts",
    "test": "jest",
    "update-routes": "tsx scripts/update_allowed_routes.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "tracking",
    "express",
    "prisma",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ai-sdk/openai": "^1.3.23",
    "@ai-sdk/openai-compatible": "^1.0.11",
    "@aws-sdk/client-s3": "^3.856.0",
    "@aws-sdk/client-ses": "^3.856.0",
    "@aws-sdk/s3-request-presigner": "^3.856.0",
    "@clerk/clerk-sdk-node": "^4.13.23",
    "@clerk/express": "^1.7.4",
    "@openrouter/ai-sdk-provider": "^1.1.2",
    "@pinecone-database/pinecone": "^6.1.2",
    "@tsw/prisma": "workspace:*",
    "@types/node": "^20.4.0",
    "@types/stripe": "^8.0.416",
    "ai": "^5.0.22",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "chalk": "^5.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.2",
    "express-winston": "^4.2.0",
    "file-type": "^21.0.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.0",
    "openai": "^5.10.3",
    "stripe": "^18.3.0",
    "svix": "^1.69.0",
    "tsx": "^3.14.0",
    "uuid": "^9.0.1",
    "web-push": "^3.6.7",
    "winston": "^3.10.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.8",
    "@types/uuid": "^9.0.8",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "jest": "^29.6.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.1.6"
  }
}
